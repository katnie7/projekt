import random
tablica = []
for i in range(4):
  tablica.append(random.randint(1,8))
print(tablica)







def sprawdzenie(lista):
  if lista == []:
    return 0
  else:
    if lista[0]!= 1 and lista[0] != 2 and lista[0] != 3 and lista[0]!= 4 and lista[0]!= 5 and lista[0]!= 6 and lista[0]!= 7 and lista[0]!= 8:
      return 1
    else:
      return 0 + sprawdzenie(lista[1:])

###for i in range(10):


print ("Wpisz 4 cyfry")
pusta = []
for i in range(4):
  zmienna = int(input())
  pusta.append(zmienna)
print(pusta)




if sprawdzenie(pusta) == 1:
  print("nieprawidłowy element")
else:
  punkty = 0
  for i in range(len(pusta)):
    if pusta[i] == tablica[i]:
      punkty+=1
      print("X")
    else:
      for j in range(len(tablica)):
         if tablica[j]== pusta[i]:
           print("o")
  if punkty == 4:
    print("WYGRAŁEŚ!")

           
           
           
